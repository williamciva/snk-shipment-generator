{
  "name": "arquivoremessasnk",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "bin": "build/index.js",
  "scripts": {
    "start:dev": "npx nodemon",
    "build": "tsc",
    "build:exe": "pkg . && npm run copyDependeciesFiles",
    "build:clear": "rimraf ./build",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf node_modules",
    "reinstall": "npm run clean & npm install",
    "copyDependeciesFiles": "copyfiles \"./db/**/*.json\" \".env\" Sankhya_Arquivo_Remessa"
  },
  "author": "williamciva",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.15.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.21",
    "pkg": "^5.8.1",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "boxen": "4.0",
    "chalk": "2.4",
    "minimist": "^1.2.8"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "node_modules/**/*"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "Sankhya_Arquivo_Remessa",
    "options": [
      "no-bytecode"
    ]
  }
}
